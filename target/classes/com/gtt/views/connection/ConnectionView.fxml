<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label text="github Time Tracker" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <VBox prefHeight="183.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Github Login" />
            <TextField id="userLogin" />
            <Label text="Github password" />
            <PasswordField id="userPwd" />
            <Button id="submit" mnemonicParsing="false" text="Button" />
            <CheckBox id="prefLogin" mnemonicParsing="false" text="Save login" />
            <Label id="error" text="Label" visible="false" />
         </children>
      </VBox>
   </center>
   <bottom>
      <ProgressBar id="loadingBar" prefWidth="200.0" visible="false" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>

            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="75.0" text="Project :" />
                  <ChoiceBox prefWidth="150.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="75.0" text="Issue :" />
                  <TextField />
               </children>
            </HBox>
            <Label prefWidth="75.0" text="Description :" />
            <TextArea prefHeight="200.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </TextArea>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</fx:root>

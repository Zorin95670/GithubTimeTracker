                     <padding>
                        <Insets bottom="15.0" top="15.0" />
                     </padding>
                  </CalendarTextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="75.0" text="Start :" />
                  <CalendarTimeTextField>
                     <padding>
                        <Insets bottom="15.0" top="15.0" />
                     </padding>
                  </CalendarTimeTextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label prefWidth="75.0" text="End :" />
                  <CalendarTimeTextField>
                     <padding>
                        <Insets bottom="15.0" top="15.0" />
                     </padding>
                  </CalendarTimeTextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" prefWidth="200.0">
